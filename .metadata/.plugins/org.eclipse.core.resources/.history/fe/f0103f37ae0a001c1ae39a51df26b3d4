#include <stdio.h>
#include <stdlib.h>

int Factorial(int min);
int main(void)
{
	setbuf(stdout, NULL);

	int numeroIngresado;
	numeroIngresado = Factorial("ingrese numero para factorizar ");
	printf("la edad es: %d", numeroIngresado);

	return EXIT_SUCCESS;
}

